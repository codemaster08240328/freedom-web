{
  "scripts": {
    "test": "jest",
    "watch:test": "jest --watch",
    "dev": "nodemon server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node server --production",
    "export": "next build && next export",
    "lint": "yarn run lint:ts && yarn run lint:styles",
    "lint:ts": "tslint --fix --project tsconfig.server.json --config ./tslint.json",
    "lint:styles": "stylelint src/**/*",
    "test-coverage-report": "yarn http-server ./coverage/lcov-report",
    "build-image": "./dev/build.ts",
    "push": "./dev/push.ts",
    "apply": "kubectl apply -f ./dist/deploy/deploy.yml",
    "deploy-local": "run(){ yarn build $1 && yarn push; }; run",
    "deploy-staging": "run(){ yarn build-image &&  yarn push && yarn apply; }; run"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "dependencies": {
